Hidesho.Name = "Hide/sho"
Hidesho.Parent = drag1
Hidesho.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Hidesho.BorderSizePixel = 0
Hidesho.Position = UDim2.new(-0.000920147693, 0, 0.0396149084, 0)
Hidesho.Size = UDim2.new(0.194787949, 0, 0.880065262, 0)
Hidesho.Font = Enum.Font.SourceSans
Hidesho.Text = "Hide Size Changer"
Hidesho.TextColor3 = Color3.fromRGB(255, 255, 255)
Hidesho.TextScaled = true
Hidesho.TextSize = 14.000
Hidesho.TextWrapped = true
function Mini3()
	if Hidesho.Text == "Hide Size Changer" then
		Hidesho.Text = "Show Size Changer"
		Setsize.Visible = false
		Sizechange.Visible = false
		Sizechange1.Visible = false
	elseif Hidesho.Text == "Show Size Changer" then
		Hidesho.Text = "Hide Size Changer"
		Setsize.Visible = true
		Sizechange.Visible = true
		Sizechange1.Visible = true
	end
end
Hidesho.MouseButton1Click:Connect(Mini3)


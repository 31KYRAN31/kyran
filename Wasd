Miniize.Name = "Miniize"
Miniize.Parent = drag
Miniize.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Miniize.BorderSizePixel = 0
Miniize.Position = UDim2.new(0.787025094, 0, -0.0297756195, 0)
Miniize.Size = UDim2.new(0.104525521, 0, 1.00213039, 0)
Miniize.Font = Enum.Font.SourceSans
Miniize.Text = "-"
Miniize.TextColor3 = Color3.fromRGB(255, 255, 255)
Miniize.TextSize = 50.000
Miniize.TextWrapped = true
function Mini()
	if Miniize.Text == "-" then
		Miniize.Text = "+"
		Frame.Visible = false
	elseif Miniize.Text == "+" then
		Miniize.Text = "-"
		Frame.Visible = true
	end
end
Miniize.MouseButton1Click:Connect(Mini)

